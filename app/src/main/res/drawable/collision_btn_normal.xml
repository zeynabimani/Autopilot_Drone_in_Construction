<?xml version="1.0" encoding="UTF-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false" >
        <layer-list>
            <item>
                <shape android:shape="oval">
                    <solid android:color="#fff"/>
                    <stroke android:width="1dp" android:color="#1a1c20"/>
                </shape>
            </item>
            <item
                android:drawable="@drawable/collision"
                android:bottom="20dp"
                android:left="20dp"
                android:right="20dp"
                android:top="20dp"/>
        </layer-list>
    </item>
</selector>
